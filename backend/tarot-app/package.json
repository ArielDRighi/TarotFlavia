{
  "name": "tarot-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --no-watchman --all",
    "test:watch": "jest --watch --no-watchman",
    "test:cov": "jest --coverage --no-watchman --all",
    "test:cov:html": "jest --coverage --no-watchman --all && open coverage/index.html",
    "test:cov:summary": "jest --coverage --no-watchman --all --coverageReporters=text-summary",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --no-watchman --all",
    "test:e2e": "jest --config ./test/jest-e2e.json --no-watchman",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --watch --no-watchman",
    "test:e2e:cov": "jest --config ./test/jest-e2e.json --coverage --no-watchman",
    "test:e2e:cov:html": "jest --config ./test/jest-e2e.json --coverage --no-watchman && open coverage-e2e/index.html",
    "test:all:cov": "npm run test:cov && npm run test:e2e:cov",
    "test:coverage": "npm run test:cov",
    "test:mvp": "jest --config ./test/jest-e2e.json test/mvp-complete.e2e-spec.ts --no-watchman",
    "seed": "ts-node -r tsconfig-paths/register src/database/seeds/seed-data.ts",
    "clean:test-users": "ts-node -r tsconfig-paths/register scripts/clean-test-users.ts",
    "db:reset": "npm run db:dev:reset",
    "db:seed:all": "ts-node -r tsconfig-paths/register scripts/db-seed-all.ts",
    "db:seed:cards": "ts-node -r tsconfig-paths/register scripts/db-seed-cards.ts",
    "db:seed:users": "ts-node -r tsconfig-paths/register scripts/db-seed-users.ts",
    "generate:reading": "ts-node -r tsconfig-paths/register scripts/generate-reading.ts",
    "test:e2e:local": "npm run db:e2e:reset && npm run test:e2e && npm run db:e2e:clean",
    "logs:openai": "ts-node -r tsconfig-paths/register scripts/logs-openai.ts",
    "stats:cache": "ts-node -r tsconfig-paths/register scripts/stats-cache.ts",
    "cli": "ts-node -r tsconfig-paths/register scripts/cli.ts",
    "migration:generate": "node ../../node_modules/typeorm/cli-ts-node-commonjs.js migration:generate -d src/config/data-source.ts",
    "migration:create": "node ../../node_modules/typeorm/cli-ts-node-commonjs.js migration:create",
    "migration:run": "node ../../node_modules/typeorm/cli-ts-node-commonjs.js migration:run -d src/config/data-source.ts",
    "migration:revert": "node ../../node_modules/typeorm/cli-ts-node-commonjs.js migration:revert -d src/config/data-source.ts",
    "migration:show": "node ../../node_modules/typeorm/cli-ts-node-commonjs.js migration:show -d src/config/data-source.ts",
    "migrate:historical-data": "ts-node -r tsconfig-paths/register run-migration.ts",
    "db:dev:clean": "bash scripts/db-dev-clean.sh",
    "db:dev:reset": "bash scripts/db-dev-reset.sh",
    "db:dev:clean:win": "powershell -ExecutionPolicy Bypass -File scripts/db-dev-clean.ps1",
    "db:dev:reset:win": "powershell -ExecutionPolicy Bypass -File scripts/db-dev-reset.ps1",
    "db:e2e:clean": "bash scripts/db-e2e-clean.sh",
    "db:e2e:reset": "bash scripts/db-e2e-reset.sh",
    "db:e2e:clean:win": "powershell -ExecutionPolicy Bypass -File scripts/db-e2e-clean.ps1",
    "db:e2e:reset:win": "powershell -ExecutionPolicy Bypass -File scripts/db-e2e-reset.ps1",
    "db:e2e:migrate": "cross-env DATABASE_URL=postgresql://${TAROT_E2E_DB_USER}:${TAROT_E2E_DB_PASSWORD}@localhost:${TAROT_E2E_DB_PORT}/${TAROT_E2E_DB_NAME} npm run migration:run",
    "pretest:e2e": "echo 'E2E tests use global setup/teardown - no pre-reset needed'",
    "test:e2e:fresh": "npm run db:e2e:reset && npm run test:e2e",
    "validate:schema": "npm run build && tsc scripts/validate-schema-consistency.ts --outDir dist/scripts --module commonjs --esModuleInterop && node dist/scripts/validate-schema-consistency.js"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.0.6",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "axios": "^1.8.1",
    "bcryptjs": "^3.0.2",
    "cache-manager": "^7.2.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "fast-levenshtein": "^2.0.6",
    "groq-sdk": "^0.34.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "nodemailer": "^7.0.10",
    "openai": "^4.86.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.17.2",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.27",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/fast-levenshtein": "^0.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/nodemailer": "^7.0.3",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pdfkit": "^0.17.3",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^6.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.spec.ts",
      "!src/**/*.e2e-spec.ts",
      "!src/**/index.ts",
      "!src/main.ts",
      "!src/config/**",
      "!src/database/migrations/**",
      "!src/database/seeds/**"
    ],
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "text",
      "text-summary",
      "html",
      "lcov",
      "json"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 70,
        "branches": 55,
        "functions": 65,
        "statements": 70
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "/dist/",
      ".module.ts$",
      "main.ts$"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "watchman": false
  }
}
